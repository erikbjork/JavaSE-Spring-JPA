	<modelVersion>4.0.0</modelVersion>

	<groupId>se.ebjork</groupId>
	<artifactId>javase-spring-jpa</artifactId>
	<version>1.0-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>JavaSE-Spring-JPA</name>
	<url>http://ebjork.se</url>

	<developers>
		<developer>
			<name>Erik Bj√∂rk</name>
